pipeline {
  agent any
  stages {
    stage("list"){
      steps {
        sh 'echo "hello world"'
        sh 'ls -a'
      }
  }
    stage("mv"){
      steps {
        sh 'touch file1.txt'
        sh 'mv file1.txt file2.txt'
      }
    }
    stage("pwd"){
      steps {
        sh 'ls'
        sh 'pwd'
      }
    }
  }
}
